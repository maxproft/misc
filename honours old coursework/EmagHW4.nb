Cell[TextData[{
 "Note: all integrals are over (-\[Infinity],\[Infinity]) unless otherwise \
specified.\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Psi]", "(", 
     RowBox[{
      OverscriptBox["x", "\[RightVector]"], ",", " ", "t"}], ")"}], " ", "=", 
    " ", 
    RowBox[{"\[Integral]", 
     RowBox[{
      FractionBox[
       SubscriptBox[
        RowBox[{"[", 
         RowBox[{"f", "(", 
          RowBox[{
           RowBox[{
            OverscriptBox["x", "\[RightVector]"], "'"}], ",", 
           RowBox[{"t", "'"}]}], ")"}], "]"}], "ret"], 
       RowBox[{"|", 
        RowBox[{
         OverscriptBox["x", "\[RightVector]"], "-", 
         RowBox[{
          OverscriptBox["x", "\[RightVector]"], "'"}]}], "|"}]], 
      SuperscriptBox["d", "3"], 
      RowBox[{"x", "'"}]}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\nwhere ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"f", "(", 
     RowBox[{
      RowBox[{
       OverscriptBox["x", "\[RightVector]"], "'"}], ",", 
      RowBox[{"t", "'"}]}], ")"}], "=", 
    RowBox[{
     RowBox[{"\[Delta]", "(", 
      RowBox[{"x", "'"}], ")"}], 
     RowBox[{"\[Delta]", "(", 
      RowBox[{"t", "'"}], ")"}]}]}], TraditionalForm]]],
 "\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Implies]", 
     RowBox[{"\[Psi]", "(", 
      RowBox[{
       OverscriptBox["x", "\[RightVector]"], ",", " ", "t"}], ")"}]}], " ", 
    "=", " ", 
    RowBox[{"\[Integral]", 
     RowBox[{
      FractionBox[
       SubscriptBox["", 
        RowBox[{
         RowBox[{"\[Delta]", "(", 
          RowBox[{"x", "'"}], ")"}], 
         RowBox[{"\[Delta]", "(", 
          RowBox[{"t", "-", 
           FractionBox[
            RowBox[{"|", 
             RowBox[{
              OverscriptBox["x", "\[RightVector]"], "-", 
              RowBox[{
               OverscriptBox["x", "\[RightVector]"], "'"}]}], "|"}], "c"]}], 
          ")"}]}]], 
       RowBox[{"|", 
        RowBox[{
         OverscriptBox["x", "\[RightVector]"], "-", 
         RowBox[{
          OverscriptBox["x", "\[RightVector]"], "'"}]}], "|"}]], 
      RowBox[{"dx", "'"}], 
      RowBox[{"dy", "'"}], 
      RowBox[{"dz", "'"}]}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\ndoing the integral with respect to x\[CloseCurlyQuote] we get:\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Implies]", 
     RowBox[{"\[Psi]", "(", 
      RowBox[{
       OverscriptBox["x", "\[RightVector]"], ",", " ", "t"}], ")"}]}], " ", 
    "=", " ", 
    RowBox[{"\[Integral]", 
     RowBox[{
      FractionBox[
       SubscriptBox["", 
        RowBox[{"\[Delta]", "(", 
         RowBox[{"t", "-", 
          FractionBox[
           SqrtBox[
            RowBox[{
             SuperscriptBox["x", "2"], "+", 
             SuperscriptBox[
              RowBox[{"(", 
               RowBox[{"y", "-", 
                RowBox[{"y", "'"}]}], ")"}], "2"], "+", 
             SuperscriptBox[
              RowBox[{"(", 
               RowBox[{"z", "-", 
                RowBox[{"z", "'"}]}], ")"}], "2"]}]], "c"]}], ")"}]], 
       SqrtBox[
        RowBox[{
         SuperscriptBox["x", "2"], "+", 
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{"y", "-", 
            RowBox[{"y", "'"}]}], ")"}], "2"], "+", 
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{"z", "-", 
            RowBox[{"z", "'"}]}], ")"}], "2"]}]]], 
      RowBox[{"dy", "'"}], 
      RowBox[{"dz", "'"}]}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\nUsing ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Delta]", "(", 
     RowBox[{"g", "(", 
      RowBox[{"y", "'"}], ")"}], ")"}], "=", 
