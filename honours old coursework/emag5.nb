             SubscriptBox["q", "e"], "'"}]]}], "+", 
          RowBox[{
           RowBox[{
            StyleBox["B",
             FontWeight->"Bold"], "'"}], "Cos\[Xi]"}]}], ")"}]}], ")"}]}]}]}],
    TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    StyleBox["F",
     FontWeight->"Bold"], "=", 
    RowBox[{
     RowBox[{
      StyleBox[
       RowBox[{"E", "'"}],
       FontWeight->"Bold"], 
      SubscriptBox[
       RowBox[{"q", "'"}], "e"]}], "+", 
     RowBox[{
      StyleBox[
       RowBox[{"H", "'"}],
       FontWeight->"Bold"], 
