Notebook[{
Cell["\<\
b)
We have found the Grand Partition Function to be:\
\>", "Text",
 CellChangeTimes->{{3.6674241940524273`*^9, 3.6674242034128113`*^9}, {
  3.6674248370614223`*^9, 3.667424837773243*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"Z", "[", "\[Beta]_", "]"}], "=", 
   RowBox[{"4", 
    FractionBox["\[Pi]", 
     RowBox[{"n", "!"}]], 
    SuperscriptBox[
     RowBox[{"(", 
      FractionBox[
       RowBox[{"2", "\[Pi]", " ", "m"}], 
       RowBox[{"\[Beta]", " ", 
        SuperscriptBox["h", "2"]}]], ")"}], 
     RowBox[{"3", 
      RowBox[{"n", "/", "2"}]}]], 
    RowBox[{"Exp", "[", 
     RowBox[{"\[Mu]", " ", "\[Beta]", " ", "n"}], "]"}], 
    SuperscriptBox[
     RowBox[{"(", 
      FractionBox[
       RowBox[{"Sinh", "[", 
        RowBox[{"H", " ", "\[Beta]", " ", "\[Mu]0"}], "]"}], 
       RowBox[{"H", " ", "\[Beta]", " ", "\[Mu]0"}]], ")"}], "n"]}]}], 
  ";"}]], "Input",
 CellChangeTimes->{{3.667424100880822*^9, 3.6674241729489603`*^9}}],

Cell[TextData[{
 "The Grand Potential is given by:\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[CapitalOmega]", "=", 
    RowBox[{
     FractionBox[
      RowBox[{"-", "1"}], "\[Beta]"], 
     RowBox[{"Ln", "(", "Z", ")"}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\nExpanding out all of the terms we get:"
}], "Text",
 CellChangeTimes->{{3.667424181513472*^9, 3.6674242094927797`*^9}, {
  3.667424310287497*^9, 3.667424338153144*^9}, {3.66742520016565*^9, 
  3.667425208303114*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"\[CapitalOmega]", "[", "\[Beta]_", "]"}], "=", 
  RowBox[{"Expand", "[", 
   RowBox[{"PowerExpand", "[", 
    RowBox[{
     FractionBox[
      RowBox[{"-", "1"}], "\[Beta]"], 
     RowBox[{"Log", "[", 
      RowBox[{"Z", "[", "\[Beta]", "]"}], "]"}]}], "]"}], "]"}]}]], "Input",
 CellChangeTimes->{{3.667424341726056*^9, 3.667424392857095*^9}, {
  3.6674247306520767`*^9, 3.667424736789361*^9}, {3.667424775725293*^9, 
  3.667424789795967*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"-", "n"}], " ", "\[Mu]"}], "-", 
  FractionBox[
   RowBox[{"2", " ", 
    RowBox[{"Log", "[", "2", "]"}]}], "\[Beta]"], "-", 
  FractionBox[
   RowBox[{"3", " ", "n", " ", 
    RowBox[{"Log", "[", "2", "]"}]}], 
   RowBox[{"2", " ", "\[Beta]"}]], "+", 
  FractionBox[
   RowBox[{"3", " ", "n", " ", 
    RowBox[{"Log", "[", "h", "]"}]}], "\[Beta]"], "+", 
  FractionBox[
   RowBox[{"n", " ", 
    RowBox[{"Log", "[", "H", "]"}]}], "\[Beta]"], "-", 
  FractionBox[
   RowBox[{"3", " ", "n", " ", 
    RowBox[{"Log", "[", "m", "]"}]}], 
   RowBox[{"2", " ", "\[Beta]"}]], "-", 
  FractionBox[
   RowBox[{"Log", "[", "\[Pi]", "]"}], "\[Beta]"], "-", 
  FractionBox[
   RowBox[{"3", " ", "n", " ", 
    RowBox[{"Log", "[", "\[Pi]", "]"}]}], 
   RowBox[{"2", " ", "\[Beta]"}]], "+", 
  FractionBox[
   RowBox[{"5", " ", "n", " ", 
    RowBox[{"Log", "[", "\[Beta]", "]"}]}], 
   RowBox[{"2", " ", "\[Beta]"}]], "+", 
  FractionBox[
   RowBox[{"n", " ", 
    RowBox[{"Log", "[", "\[Mu]0", "]"}]}], "\[Beta]"], "+", 
  FractionBox[
   RowBox[{"Log", "[", 
    RowBox[{"n", "!"}], "]"}], "\[Beta]"], "-", 
  FractionBox[
   RowBox[{"n", " ", 
    RowBox[{"Log", "[", 
     RowBox[{"Sinh", "[", 
      RowBox[{"H", " ", "\[Beta]", " ", "\[Mu]0"}], "]"}], "]"}]}], 
   "\[Beta]"]}]], "Output",
 CellChangeTimes->{{3.667424778469857*^9, 3.667424790142683*^9}}]
}, Open  ]],

Cell[TextData[{
 "c)\nFinding the magnetism:\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"M", "=", 
    RowBox[{
     RowBox[{"-", 
      FractionBox[
       RowBox[{"\[PartialD]", "\[CapitalOmega]"}], 
       RowBox[{"\[PartialD]", "H"}]]}], "=", 
     RowBox[{"-", 
      RowBox[{
       FractionBox["\[PartialD]", 
        RowBox[{"\[PartialD]", "H"}]], 
       RowBox[{"(", 
        RowBox[{
         FractionBox[
          RowBox[{"n", " ", 
           RowBox[{"Log", "[", "H", "]"}]}], "\[Beta]"], "-", 
         FractionBox[
          RowBox[{"n", " ", 
           RowBox[{"Log", "[", 
            RowBox[{"Sinh", "[", 
             RowBox[{"H", " ", "\[Beta]", " ", "\[Mu]0"}], "]"}], "]"}]}], 
          "\[Beta]"]}], ")"}]}]}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"M", "=", 
    RowBox[{
     RowBox[{"-", 
      FractionBox["n", 
       RowBox[{"H", " ", "\[Beta]"}]]}], "+", 
     RowBox[{"n", " ", "\[Mu]0", " ", 
      RowBox[{"Coth", "[", 
       RowBox[{"H", " ", "\[Beta]", " ", "\[Mu]0"}], "]"}]}]}]}], 
   TraditionalForm]],
  FormatType->"TraditionalForm"]
}], "Text",
