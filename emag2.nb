Cell[TextData[{
 "\n\n3.9\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["\[Del]", "2"], "\[CapitalPhi]"}], "=", 
    RowBox[{
     RowBox[{"0", "\[Implies]", " ", 
      FractionBox[
       RowBox[{
        SuperscriptBox["\[Del]", "2"], "\[CapitalPhi]"}], "\[CapitalPhi]"]}], 
     "=", "0"}]}], TraditionalForm]]],
 "\n\[CapitalPhi](s,\[Phi],z)=",
 Cell[BoxData[
  FormBox[
   RowBox[{"R", "(", "s", ")"}], TraditionalForm]]],
 "T(\[Phi])Z(z)\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      FractionBox["1", 
       RowBox[{"s", " ", 
        RowBox[{"R", "(", "s", ")"}]}]], " ", 
      RowBox[{
       FractionBox["\[PartialD]", 
        RowBox[{"\[PartialD]", "s"}]], 
       RowBox[{"(", 
        RowBox[{"s", " ", 
         RowBox[{"R", "'"}], " ", 
         RowBox[{"(", "s", ")"}]}], ")"}]}]}], "+", 
     RowBox[{
      FractionBox["1", 
       RowBox[{
        SuperscriptBox["s", "2"], 
        RowBox[{"T", "(", "\[Phi]", ")"}]}]], 
      FractionBox[
       RowBox[{
        SuperscriptBox["\[PartialD]", "2"], 
        RowBox[{"T", "(", "\[Phi]", ")"}]}], 
       RowBox[{"\[PartialD]", 
        SuperscriptBox["\[Phi]", "2"]}]]}], "+", 
     RowBox[{
      FractionBox["1", 
       RowBox[{"Z", "(", "z", ")"}]], 
      FractionBox[
       RowBox[{
        SuperscriptBox["\[PartialD]", "2"], 
        RowBox[{"Z", "(", "z", ")"}]}], 
       RowBox[{"\[PartialD]", 
        SuperscriptBox["z", "2"]}]]}]}], "=", "0"}], TraditionalForm]]],
 "\n\[Implies] ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox[
     RowBox[{
      SuperscriptBox["\[PartialD]", "2"], 
      RowBox[{"Z", "(", "z", ")"}]}], 
     RowBox[{"\[PartialD]", 
      SuperscriptBox["z", "2"]}]], "=", 
    RowBox[{
     RowBox[{"-", 
      SuperscriptBox["a", "2"]}], 
     RowBox[{"Z", "(", "z", ")"}]}]}], TraditionalForm]]],
 ", for some constant a.\nThis gives solutions\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"Z", "(", "z", ")"}], "=", 
    RowBox[{
     RowBox[{"A", " ", 
      SuperscriptBox["e", "iaz"]}], "+", 
     RowBox[{"B", " ", 
      SuperscriptBox["e", 
       RowBox[{"-", "iaz"}]]}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\nThe potential is zero when z=0,L\n\[Implies] A=-B\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      SuperscriptBox["Ae", "iaL"], "-", 
      SuperscriptBox["Ae", 
       RowBox[{"-", "iaL"}]]}], "=", 
     RowBox[{
      RowBox[{"0", "\[Implies]", " ", 
       SuperscriptBox["e", 
        RowBox[{"2", "iaL"}]]}], "=", 
      RowBox[{
       RowBox[{"1", " ", "\[Implies]", " ", "a"}], "=", 
       FractionBox["k\[Pi]", "L"]}]}]}], ",", " ", 
    RowBox[{"for", " ", "a", " ", "positive", " ", "integer", " ", "k"}]}], 
   TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Implies]", 
     RowBox[{"Z", "(", "z", ")"}]}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"A", " ", 
       SuperscriptBox["e", 
        RowBox[{"i", " ", "k", " ", "\[Pi]", " ", 
         RowBox[{"z", "/", "L"}]}]]}], "-", 
      RowBox[{"A", " ", 
       SuperscriptBox["e", 
        RowBox[{
         RowBox[{"-", "i"}], " ", "k", " ", "\[Pi]", " ", 
         RowBox[{"z", "/", "L"}]}]]}]}], "=", 
     RowBox[{"2", "A", " ", 
      RowBox[{"Sin", "(", 
       RowBox[{"k\[Pi]", " ", 
        RowBox[{"z", "/", "L"}]}], ")"}]}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\nRearranging the laplacian again:\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      FractionBox["s", 
       RowBox[{" ", 
        RowBox[{"R", "(", "s", ")"}]}]], " ", 
      RowBox[{
       FractionBox["\[PartialD]", 
        RowBox[{"\[PartialD]", "s"}]], 
       RowBox[{"(", 
        RowBox[{"s", " ", 
         RowBox[{"R", "'"}], " ", 
         RowBox[{"(", "s", ")"}]}], ")"}]}]}], "+", 
     RowBox[{
      FractionBox["1", 
       RowBox[{"T", "(", "\[Phi]", ")"}]], 
      FractionBox[
       RowBox[{
        SuperscriptBox["\[PartialD]", "2"], 
        RowBox[{"T", "(", "\[Phi]", ")"}]}], 
       RowBox[{"\[PartialD]", 
        SuperscriptBox["\[Phi]", "2"]}]]}], "+", 
     RowBox[{
      SuperscriptBox["s", "2"], 
      SuperscriptBox["a", "2"]}]}], "=", "0"}], TraditionalForm]]],
 "\nAnd so we get:\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox[
     RowBox[{
      SuperscriptBox["\[PartialD]", "2"], 
      RowBox[{"T", "(", "\[Phi]", ")"}]}], 
     RowBox[{"\[PartialD]", 
      SuperscriptBox["\[Phi]", "2"]}]], "=", 
    RowBox[{
     RowBox[{"-", 
      SuperscriptBox["d", "2"]}], 
     RowBox[{"T", "(", "\[Phi]", ")"}]}]}], TraditionalForm]]],
 "\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Implies]", 
     RowBox[{"T", 
      RowBox[{"(", "\[Phi]", ")"}]}]}], "=", 
    RowBox[{
     SuperscriptBox["Ce", "id\[Phi]"], "+", 
     SuperscriptBox["De", 
      RowBox[{"-", "id\[Phi]"}]]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\nFrom continuting of the laplacian:\nT(0)=T(2\[Pi])\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Implies]", 
     RowBox[{"C", "+", "D"}]}], "=", 
    RowBox[{
     SuperscriptBox["Ce", 
      RowBox[{"id", " ", "2", "\[Pi]"}]], "+", 
     SuperscriptBox["De", 
      RowBox[{
       RowBox[{"-", "id"}], " ", "2", "\[Pi]"}]]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Implies]", 
     RowBox[{
      SuperscriptBox[
       RowBox[{"C", "(", 
        SuperscriptBox["e", 
         RowBox[{"id", " ", "2", "\[Pi]"}]], ")"}], "2"], "-", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"C", "+", "D"}], ")"}], 
       RowBox[{"(", 
        SuperscriptBox["e", 
         RowBox[{"id", " ", "2", "\[Pi]"}]], ")"}]}], "+", "D"}]}], "=", 
    "0"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["e", 
     RowBox[{"id", " ", "2", "\[Pi]"}]], "=", 
    FractionBox[
     RowBox[{"C", "+", 
      RowBox[{"D", "\[PlusMinus]", 
       SqrtBox[
        RowBox[{
         SuperscriptBox["C", "2"], "+", 
         SuperscriptBox["D", "2"], "+", 
         RowBox[{"2", "CD"}], "-", 
         RowBox[{"4", "CD"}]}]]}]}], 
     RowBox[{"2", "C"}]]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["e", 
     RowBox[{"id", " ", "2", "\[Pi]"}]], "=", 
    RowBox[{
     FractionBox[
      RowBox[{"C", "+", 
       RowBox[{"D", "\[PlusMinus]", 
        RowBox[{"(", 
         RowBox[{"C", "-", "D"}], ")"}]}]}], 
      RowBox[{"2", "C"}]], "=", 
     RowBox[{"1", " ", "OR", " ", 
      FractionBox["D", "C"]}]}]}], TraditionalForm]]],
 "\nUsing ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["e", 
     RowBox[{"id", " ", "2", "\[Pi]"}]], "=", 
    FractionBox["D", "C"]}], TraditionalForm]]],
 " (As we can take D=C to get the other solution)\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Implies]", 
     RowBox[{"T", 
      RowBox[{"(", "\[Phi]", ")"}]}]}], "=", 
    RowBox[{
     SuperscriptBox["Ce", "id\[Phi]"], "+", 
     SuperscriptBox["Ce", 
      RowBox[{"-", 
       RowBox[{"id", "(", 
        RowBox[{"\[Phi]", "-", 
         RowBox[{"2", "\[Pi]"}]}], ")"}]}]]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\nFrom the continutity of the laplacian:\nT(\[Pi])=T(-\[Pi])\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Implies]", 
     RowBox[{
      SuperscriptBox["Ce", "id\[Pi]"], "+", 
      SuperscriptBox["Ce", "id\[Pi]"]}]}], "=", 
    RowBox[{
     SuperscriptBox["Ce", 
      RowBox[{"-", "id\[Pi]"}]], "+", 
     SuperscriptBox["Ce", "id3\[Pi]"]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Implies]", "1"}], "=", 
    RowBox[{
     FractionBox[
      RowBox[{
       SuperscriptBox["e", 
        RowBox[{"-", "id2\[Pi]"}]], "+", 
       SuperscriptBox["e", "id2\[Pi]"]}], "2"], "=", 
     RowBox[{"cos", "(", 
      RowBox[{"2", "\[Pi]", " ", "d"}], ")"}]}]}], TraditionalForm]]],
 "\n\[Implies] b is a positive integer.\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Implies]", 
     RowBox[{"T", 
      RowBox[{"(", "\[Phi]", ")"}]}]}], "=", 
    RowBox[{
     RowBox[{
      SuperscriptBox["Ce", "id\[Phi]"], "+", 
      SuperscriptBox["Ce", 
       RowBox[{"-", "id\[Phi]"}]]}], "=", 
     RowBox[{"2", "C", " ", 
      RowBox[{"Cos", "(", "d\[Phi]", ")"}]}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\nRearranging the Laplacian Once More:\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      FractionBox["s", 
       RowBox[{" ", 
        RowBox[{"R", "(", "s", ")"}]}]], " ", 
      RowBox[{
       FractionBox["\[PartialD]", 
        RowBox[{"\[PartialD]", "s"}]], 
       RowBox[{"(", 
        RowBox[{"s", " ", 
         RowBox[{"R", "'"}], " ", 
         RowBox[{"(", "s", ")"}]}], ")"}]}]}], "+", 
     SuperscriptBox["d", "2"], "+", 
     RowBox[{
      SuperscriptBox["s", "2"], 
      SuperscriptBox["a", "2"]}]}], "=", "0"}], TraditionalForm]]],
 "\n",
 Cell[BoxData[
