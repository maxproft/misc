Cell["\<\
Max Proft
U5190335
Assignment 2\
\>", "Subsubsection",
 CellChangeTimes->{{3.665990186859405*^9, 3.6659901947264643`*^9}, {
  3.6660420280996428`*^9, 3.666042030890108*^9}}],

Cell[TextData[{
 "3.1\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["\[Del]", "2"], "\[CapitalPhi]"}], "=", 
    RowBox[{
     RowBox[{"0", "\[Implies]", " ", 
      RowBox[{
       FractionBox[
        RowBox[{
         SuperscriptBox["\[Del]", "2"], "\[CapitalPhi]"}], "\[CapitalPhi]"], 
       SuperscriptBox["r", "2"]}]}], "=", "0"}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\nTaking ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[CapitalPhi]", "(", 
     RowBox[{"r", ",", "\[Theta]"}], ")"}], "=", 
    RowBox[{
     FractionBox[
      RowBox[{"R", "(", "r", ")"}], "r"], 
     RowBox[{"T", "(", "\[Theta]", ")"}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " (The system is symmetrical in the phi direction)\nIn spherical coordinates \
this gives us:\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      FractionBox["1", 
       RowBox[{"R", "(", "r", ")"}]], 
      FractionBox[
       RowBox[{
        SuperscriptBox["\[PartialD]", "2"], 
        RowBox[{"R", "(", "r", ")"}]}], 
       RowBox[{"\[PartialD]", 
        SuperscriptBox["r", "2"]}]]}], "+", 
     RowBox[{
      FractionBox["1", 
       RowBox[{
        RowBox[{"T", "(", "\[Theta]", ")"}], 
        RowBox[{"sin", "(", "\[Theta]", ")"}]}]], 
      FractionBox["d", "d\[Theta]"], 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"sin", "(", "\[Theta]", ")"}], " ", 
        FractionBox[
         RowBox[{"dT", "(", "\[Theta]", ")"}], "d\[Theta]"]}], ")"}]}]}], "=",
     "0", " "}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\nSince both terms are independent of each other, we must get that:\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     FractionBox[
      SuperscriptBox["r", "2"], 
      RowBox[{"R", "(", "r", ")"}]], 
     FractionBox[
      RowBox[{
       SuperscriptBox["\[PartialD]", "2"], 
       RowBox[{"R", "(", "r", ")"}]}], 
      RowBox[{"\[PartialD]", 
       SuperscriptBox["r", "2"]}]]}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"-", 
       FractionBox["1", 
        RowBox[{
         RowBox[{"T", "(", "\[Theta]", ")"}], 
         RowBox[{"sin", "(", "\[Theta]", ")"}]}]]}], 
      FractionBox["d", "d\[Theta]"], 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"sin", "(", "\[Theta]", ")"}], " ", 
        FractionBox[
         RowBox[{"dT", "(", "\[Theta]", ")"}], "d\[Theta]"]}], ")"}]}], "=", 
     RowBox[{"Constant", "=", 
      RowBox[{"l", "(", 
       RowBox[{"l", "+", "1"}], ")"}], " "}]}]}], TraditionalForm]]],
 "\nSolving for R(r) we get:\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox[
     RowBox[{
      SuperscriptBox["\[PartialD]", "2"], 
      RowBox[{"R", "(", "r", ")"}]}], 
     RowBox[{"\[PartialD]", 
      SuperscriptBox["r", "2"]}]], "=", " ", 
    RowBox[{
     RowBox[{"l", "(", 
      RowBox[{"l", " ", "+", " ", "1"}], ")"}], " ", 
     FractionBox[
      RowBox[{"R", "(", "r", ")"}], 
      SuperscriptBox["r", "2"]]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\nWhich has solutions:\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"R", "(", "r", ")"}], "=", 
    RowBox[{
     SuperscriptBox["Ar", 
      RowBox[{"l", "+", "1"}]], "+", 
     SuperscriptBox["Br", 
      RowBox[{"-", "l"}]]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\nSolving for T(\[Theta]) we get:\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      FractionBox["d", "d\[Theta]"], 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"sin", "(", "\[Theta]", ")"}], " ", 
        FractionBox[
         RowBox[{"dT", "(", "\[Theta]", ")"}], "d\[Theta]"]}], ")"}]}], "+", 
     RowBox[{
      RowBox[{"l", "(", 
       RowBox[{"l", "+", "1"}], ")"}], 
      RowBox[{"T", "(", "\[Theta]", ")"}], 
      RowBox[{"sin", "(", "\[Theta]", ")"}]}]}], "=", "0"}], 
   TraditionalForm]]],
 "\nLet x=cos(\[Theta]) \[Implies] dx = -sin(\[Theta]) d\[Theta] (Note: 0<\
\[Theta]<\[Pi] \[Implies] -1<x<1) \nSubstituting in for x we get:\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{"-", 
       RowBox[{"sin", "(", "\[Theta]", ")"}]}], 
      FractionBox["d", "dx"], 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"-", 
         RowBox[{
          SuperscriptBox["sin", "2"], "(", "\[Theta]", ")"}]}], " ", 
        RowBox[{"T", "'"}], 
        RowBox[{"(", "x", ")"}]}], " ", ")"}]}], "+", 
     RowBox[{
      RowBox[{"l", "(", 
       RowBox[{"l", "+", "1"}], ")"}], 
      RowBox[{"T", "(", "x", ")"}], 
      RowBox[{"sin", "(", "\[Theta]", ")"}]}]}], "=", "0"}], 
   TraditionalForm]]],
 "\nSince ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"1", "-", 
     SuperscriptBox["x", "2"]}], "=", 
    RowBox[{
     RowBox[{"1", "-", 
      RowBox[{
       SuperscriptBox["cos", "2"], "(", "\[Theta]", ")"}]}], "=", 
     RowBox[{
      SuperscriptBox["sin", "2"], "(", "\[Theta]", ")"}]}]}], 
   TraditionalForm]],
  FormatType->"TraditionalForm"],
 " we get:\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      FractionBox["d", "dx"], 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"1", "-", 
          SuperscriptBox["x", "2"]}], ")"}], " ", 
        RowBox[{"T", "'"}], 
        RowBox[{"(", "x", ")"}]}], " ", ")"}]}], "+", 
     RowBox[{
      RowBox[{"l", "(", 
       RowBox[{"l", "+", "1"}], ")"}], 
      RowBox[{"T", "(", "x", ")"}]}]}], "=", "0"}], TraditionalForm]]],
 "\nThis has solutions ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["U", "l"], "(", "x", ")"}], "=", 
    RowBox[{
     FractionBox["1", 
      RowBox[{
       SuperscriptBox["2", "l"], 
       RowBox[{"l", "!"}]}]], 
     FractionBox[
      SuperscriptBox["d", "l"], 
      SuperscriptBox["dx", "l"]], 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{
        SuperscriptBox["x", "2"], "-", "1"}], ")"}], "l"]}]}], 
   TraditionalForm]],
  FormatType->"TraditionalForm"],
 ", where ",
 Cell[BoxData[
  FormBox["l", TraditionalForm]]],
 " is a non-negative integer.\nSo we can write the general solution as:\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[CapitalPhi]", 
     RowBox[{"(", 
      RowBox[{"r", ",", "\[Theta]"}], ")"}]}], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox[
       SuperscriptBox["\[Sum]", "\[Infinity]"], 
       RowBox[{"l", "=", "0"}]], "  ", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          SubscriptBox["A", "l"], 
          SuperscriptBox["r", "l"]}], "+", 
         RowBox[{
          SubscriptBox["B", "l"], 
          SuperscriptBox["r", 
           RowBox[{
            RowBox[{"-", "l"}], "-", "1"}]]}]}], ")"}], 
       SubscriptBox["U", "l"]}]}], "=", " ", 
     RowBox[{
      SubscriptBox[
       SuperscriptBox["\[Sum]", "\[Infinity]"], 
       RowBox[{"l", "=", "0"}]], "  ", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          SubscriptBox["A", "l"], 
          SuperscriptBox["r", "l"]}], "+", 
         RowBox[{
          SubscriptBox["B", "l"], 
          SuperscriptBox["r", 
           RowBox[{
            RowBox[{"-", "l"}], "-", "1"}]]}]}], ")"}], 
       FractionBox["1", 
        RowBox[{
         SuperscriptBox["2", "l"], 
         RowBox[{"l", "!"}]}]], 
       FractionBox[
        SuperscriptBox["d", "l"], 
        SuperscriptBox["dx", "l"]], 
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{
          SuperscriptBox["x", "2"], "-", "1"}], ")"}], "l"]}]}]}]}], 
   TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\nIf we multiply both sides by ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["U", 
     RowBox[{"l", "'"}]], "(", "x", ")"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " and integrate over x (using the orthogonality of the solution):\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      SubscriptBox["A", "l"], 
      SuperscriptBox["r", "l"]}], "+", 
     RowBox[{
      SubscriptBox["B", "l"], 
      SuperscriptBox["r", 
       RowBox[{
        RowBox[{"-", "l"}], "-", "1"}]]}]}], "=", 
    RowBox[{
     FractionBox[
      RowBox[{
       RowBox[{"2", "l"}], "+", "1"}], "2"], 
     RowBox[{
      SuperscriptBox[
       SubscriptBox["\[Integral]", 
        RowBox[{"-", "1"}]], "1"], 
      RowBox[{
       RowBox[{"V", "(", 
        RowBox[{"r", ",", "x"}], ")"}], 
       FractionBox["1", 
        RowBox[{
         SuperscriptBox["2", "l"], 
         RowBox[{"l", "!"}]}]], 
       FractionBox[
        SuperscriptBox["d", "l"], 
        SuperscriptBox["dx", "l"]], 
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{
          SuperscriptBox["x", "2"], "-", "1"}], ")"}], "l"], "dx"}]}]}]}], 
   TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\nV(a,x)={",
 Cell[BoxData[GridBox[{
    {"V", 
     RowBox[{"0", "<", "x", "<", "1"}]},
    {"0", "otherwise"}
   }]]],
 "\nAnd hence:\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      SubscriptBox["A", "l"], 
      SuperscriptBox["a", "l"]}], "+", 
     RowBox[{
      SubscriptBox["B", "l"], 
      SuperscriptBox["a", 
       RowBox[{
        RowBox[{"-", "l"}], "-", "1"}]]}]}], "=", 
    RowBox[{"V", 
     FractionBox[
      RowBox[{
       RowBox[{"2", "l"}], "+", "1"}], "2"], 
     RowBox[{
      SuperscriptBox[
       SubscriptBox["\[Integral]", "0"], "1"], 
      RowBox[{
       FractionBox["1", 
        RowBox[{
         SuperscriptBox["2", "l"], 
         RowBox[{"l", "!"}]}]], 
       FractionBox[
        SuperscriptBox["d", "l"], 
        SuperscriptBox["dx", "l"]], 
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{
          SuperscriptBox["x", "2"], "-", "1"}], ")"}], "l"], "dx"}]}]}]}], 
   TraditionalForm]]],
 "\nV(b,x)={",
 Cell[BoxData[GridBox[{
    {"0", 
     RowBox[{"0", "<", "x", "<", "1"}]},
    {"V", "otherwise"}
   }]]],
 "\nAnd hence:\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      SubscriptBox["A", "l"], 
      SuperscriptBox["b", "l"]}], "+", 
     RowBox[{
      SubscriptBox["B", "l"], 
      SuperscriptBox["b", 
       RowBox[{
        RowBox[{"-", "l"}], "-", "1"}]]}]}], "=", 
    RowBox[{
     FractionBox[
      RowBox[{
       RowBox[{"2", "l"}], "+", "1"}], "2"], "V", 
     RowBox[{
      SuperscriptBox[
       SubscriptBox["\[Integral]", 
        RowBox[{"-", "1"}]], "0"], 
      RowBox[{
       FractionBox["1", 
        RowBox[{
         SuperscriptBox["2", "l"], 
         RowBox[{"l", "!"}]}]], 
       FractionBox[
        SuperscriptBox["d", "l"], 
        SuperscriptBox["dx", "l"]], 
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{
          SuperscriptBox["x", "2"], "-", "1"}], ")"}], "l"], "dx"}]}]}]}], 
   TraditionalForm]]],
 "\nSolving the for a and b with mathematica we get:"
}], "Text",
